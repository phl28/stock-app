{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"allowJs": true,
		"checkJs": false,
		"isolatedModules": true,
		"strictNullChecks": true,
		"strict": true
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.js",
		"src/**/*.svelte",
		".eslintrc.cjs",
		".svelte-kit/ambient.d.ts"
	]
}
